@model CarouselItem
@{
    ViewData["Title"] = "Edit Carousel item";
}
<h1>@ViewData["Title"]</h1>

@{
    ViewBag.Action = nameof(CarouselController.Edit);
}

@section Styles
{
    <link rel="stylesheet" href="~/css/homepage-styles.css">
}

<partial name="CreateEditWithForm" model="Model">
    @section Scripts
    {
        <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
        <script src="~/js/FileContentTypeValidation.js"></script>
    }
